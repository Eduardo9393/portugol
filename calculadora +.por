programa {
  funcao inicio() {
    real a, b, soma, sub, mult, div

    escreva ("digita o primeiro numro ai: ")
    leia (a)

    escreva ("digita outro numero ai: ")
    leia (b)
    soma = a + b 
    sub = a - b 
    mult = a * b
    div = a / b 
    escreva ("a soma dos numeros escolhidos é " ,soma)
    escreva ("\na subtração dos numeros escolhidos é " ,sub)
    escreva ("\na multiplicação dos numeros escolhidos é " ,mult)
    escreva ("\na divisão dos numeros escolhidos é " ,div)

  }
}
